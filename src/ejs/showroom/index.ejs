  <script src="../assets/js/index.js?20221206"></script>
  <script type="text/javascript">
    $(function(){
        var winScrollTop;
        // var videoElement = document.getElementsByTagName("video");
        $('.js_modal_open').each(function(){
            $(this).on('click',function(){
                //スクロール位置を取得
                winScrollTop = $(window).scrollTop();
                var target = $(this).data('target');
                var videoId = $(this).data('video');
                var modal = document.getElementById(target);
                var modalChild = $(modal).find("video");
                $(modal).fadeIn();
                $(modalChild).attr("src", videoId );
                $(".showroom__movie-wrap").addClass('show');
                return false;
            });
        });
        $('.js_modal_close').on('click',function(){
            $('.js_modal').fadeOut();
            $(".common__movie-play-tumb").show();
            $(".showroom__movie-wrap").removeClass('show');
            $("video").attr("src", "" );
            $('body,html').stop().animate({scrollTop:winScrollTop}, 100);
            return false;
        }); 
    });
  </script>
